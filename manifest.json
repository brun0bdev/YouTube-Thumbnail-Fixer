{
    "manifest_version": 3,
    "name": "YouTube Thumbnail Fixer",
    "version": "1.1.1",
    "description": "Restores the old YouTube thumbnail size.",
    "author": "BrunoB",
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["scripts/content.js"]
      }
    ]
  }